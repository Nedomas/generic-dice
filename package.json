{
  "name": "generic-dice",
  "version": "1.1.5",
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "description": "A generic dice UI component rendered with React.js",
  "main": "reactSource.js",
  "scripts": {
    "build": "npm run generic-dice && npm run examples",
    "lint": "node ./node_modules/.bin/eslint .",
    "generic-dice": "node ./node_modules/webpack/bin/webpack.js --config ./webpack.config.js --progress --colors",
    "examples": "node ./node_modules/webpack/bin/webpack.js --config ./examples/webpack.config.js --progress --colors",
    "dev": "node ./node_modules/webpack/bin/webpack.js -w --config ./webpack.config.js --progress --colors & node ./node_modules/webpack/bin/webpack.js -w --config ./examples/webpack.config.js --progress --colors",
    "test": "jest ./src/ -u"
  },
  "jest": {
    "testPathDirs": [
      "src/react/",
      "src/vanilla/"
    ],
    "scriptPreprocessor": "./scripts/jest/preprocessor.js",
    "automock": false,
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/babel-runtime"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 55,
        "functions": 90,
        "lines": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gyroscopico/generic-dice.git"
  },
  "keywords": [
    "game",
    "dice",
    "gambling",
    "react.js"
  ],
  "author": "Ernesto Paniagua <ernestopani@gmail.com> (http://gyroscopic.info)",
  "contributors": [
    {
      "name": "Nadjib Amar",
      "email": "hello@ebabel.eu",
      "url": "http://ebabel.eu"
    }
  ],
  "license": "GPL-3.0",
  "private": false,
  "dependencies": {
    "react": "15.3.1",
    "react-dom": "15.3.1"
  },
  "devDependencies": {
    "babel-core": "~6.14.0",
    "babel-eslint": "6.0.2",
    "babel-jest": "14.1.0",
    "babel-loader": "~6.2.5",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "~6.13.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-jest": "14.1.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.7.2",
    "classnames": "2.2.3",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "eslint": "~2.10.2",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "4.2.3",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "install": "~0.8.1",
    "jest": "14.1.0",
    "jest-cli": "14.1.0",
    "left-pad": "azer/left-pad",
    "node-sass": "3.8.0",
    "npm": "~3.10.6",
    "pug": "2.0.0-beta6",
    "pug-loader": "2.3.0",
    "raf.js": "0.0.4",
    "react-addons-test-utils": "~15.3.1",
    "sass-loader": "~4.0.1",
    "style-loader": "~0.13.1",
    "url-loader": "0.5.7",
    "webpack": "~1.13.2",
    "webpack-cleanup-plugin": "0.3.1"
  }
}
